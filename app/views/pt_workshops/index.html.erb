<p id="notice"><%= notice %></p>

<h1>Pt Workshops</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Person</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pt_workshops.each do |pt_workshop| %>
      <tr>
        <td><%= pt_workshop.date %></td>
        <td><%= pt_workshop.Person %></td>
        <td><%= link_to 'Show', pt_workshop %></td>
        <td><%= link_to 'Edit', edit_pt_workshop_path(pt_workshop) %></td>
        <td><%= link_to 'Destroy', pt_workshop, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pt Workshop', new_pt_workshop_path %>
