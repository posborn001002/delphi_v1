<p id="notice"><%= notice %></p>

<h1>Job Progresses</h1>

<table>
  <thead>
    <tr>
      <th>Customerjob</th>
      <th>Actionresult</th>
      <th>Lookup</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_progresses.each do |job_progress| %>
      <tr>
        <td><%= job_progress.CustomerJob %></td>
        <td><%= job_progress.ActionResult %></td>
        <td><%= job_progress.Lookup %></td>
        <td><%= link_to 'Show', job_progress %></td>
        <td><%= link_to 'Edit', edit_job_progress_path(job_progress) %></td>
        <td><%= link_to 'Destroy', job_progress, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job Progress', new_job_progress_path %>
